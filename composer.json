{
    "name": "elgorm/yii2-uploadable-cropable-image",
    "description": "Yii2 extension for upload and crop images",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "jcrop",
        "image",
        "image upload",
        "image crop"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Elgorm",
            "email": "elgorm@4ml.su"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.0.*",
        "yiisoft/yii2-imagine": "2.*",
        "yiisoft/yii2-bootstrap": "2.*",
        "bower-asset/Jcrop": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "elgorm\\image\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
